{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Express TypeScript Boilerplate",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "worker": "ts-node src/workers/stockVerificationWorker.ts",
    "worker:prod": "node dist/workers/stockVerificationWorker.js",
    "worker:stock": "ts-node src/workers/stockVerificationWorker.ts",
    "workers": "ts-node src/workers/workerManager.ts",
    "workers:prod": "node dist/workers/workerManager.js",
    "prisma:studio": "prisma studio",
    "seed": "ts-node src/scripts/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "boilerplate"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jose": "^6.0.6",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "tslib": "^2.6.2",
    "winston": "^3.17.0",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.4.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "bcrypt",
      "prisma"
    ],
    "overrides": {
      "@humanwhocodes/config-array": "^0.11.14",
      "@humanwhocodes/object-schema": "^1.2.1",
      "are-we-there-yet": "^3.0.1",
      "gauge": "^5.0.2",
      "glob": "^8.1.0",
      "inflight": "^1.0.6",
      "npmlog": "^7.0.1",
      "rimraf": "^4.4.1",
      "superagent": "^8.1.2"
    }
  }
}