{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate backend Express.js en TypeScript avec Stripe et JWT (JOSE) sous Docker multi-stage",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "dev": "prisma generate && ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest --coverage",
    "swagger-autogen": "node swagger.js",
    "seed": "ts-node src/scripts/seed.ts",
    "prisma:generate": "prisma generate",
    "worker:stock": "ts-node src/workers/stockVerificationWorker.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.4.1",
    "@prisma/client": "^6.4.1",
    "@types/dotenv": "^8.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "amqplib": "^0.10.5",
    "and": "^0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.2.1",
    "helmet": "^6.0.1",
    "ioredis": "^5.3.2",
    "jose": "^4.15.9",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "stripe": "^10.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.13.5",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "prisma": "^6.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "esbuild",
      "prisma"
    ]
  }
}